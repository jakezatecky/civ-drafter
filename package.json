{
  "name": "react-boilerplate",
  "version": "2.0.0",
  "description": "A sensible, minimal, and server-agnostic boilerplate for React projects.",
  "main": "gulpfile.js",
  "scripts": {
    "build": "webpack",
    "dev-server": "webpack serve",
    "release": "npm run test && npm run build",
    "test": "npm run test-script-format && npm run test-script-mocha && npm run test-style",
    "test-script-format": "eslint src/js/**/*.js test/**/*.js *.js",
    "test-script-mocha": "mocha --require @babel/register --require global-jsdom/register --exit --reporter spec test/**/*.js",
    "test-style": "stylelint src/scss/**/*.scss"
  },
  "author": "Jake Zatecky",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@testing-library/react": "^13.4.0",
    "babel-loader": "^9.0.0",
    "chai": "^4.2.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.27.0",
    "eslint-config-takiyon-react": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "global-jsdom": "^8.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.1",
    "stylelint-config-takiyon": "^2.0.5",
    "stylelint-webpack-plugin": "^3.3.0",
    "webpack": "^5.3.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
